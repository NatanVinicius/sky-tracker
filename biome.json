{
	"$schema": "https://biomejs.dev/schemas/1.9.4/schema.json",
	"vcs": {
	  "enabled": false,
	  "clientKind": "git",
	  "useIgnoreFile": false
	},
	"files": {
	  "ignoreUnknown": false,
	  "ignore": []
	},
	"formatter": {
	  "enabled": true,
	  "lineWidth": 65,
	  "indentWidth": 2,
	  "lineEnding": "lf",
	  "indentStyle": "tab"
	},
	"organizeImports": {
	  "enabled": true
	},
	"linter": {
	  "enabled": true,
	  "rules": {
		"recommended": true
	  }
	},
	"javascript": {
	  "formatter": {
		"semicolons": "always",
		"quoteStyle": "single",
		"arrowParentheses": "always",
		"bracketSameLine": false,
		"bracketSpacing": true,
		"trailingCommas": "all",
		"jsxQuoteStyle": "single"
	  }
	}
  }
  